   "source": [
    "for _ in range(int(input())):\n",
    "    n = list(map(int,input().split(' ')))\n",
    "    m_mean = (sum(n[1:])/n[0])\n",
    "    cnt = 0\n",
    "    for i in n[1:]:\n",
    "        if i > m_mean:\n",
    "            cnt += 1\n",
    "    per = (cnt/n[0])*100\n",
    "    print(f'{per:.3f}%')"
   ]
  }
