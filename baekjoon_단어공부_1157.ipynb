    "word = input().upper()\n",
    "word_unique = list(set(word))\n",
    "word_count = []\n",
    "for i in word_unique:\n",
    "    word_count.append(word.count(i))\n",
    "\n",
    "if word_count.count(max(word_count)) >1:\n",
    "    print('?')\n",
    "else:\n",
    "    index = word_count.index(max(word_count))\n",
    "    print(word_unique[index])"
   ]
